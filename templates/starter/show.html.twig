{% extends 'base.html.twig' %}

{% block title %}Starter{% endblock %}

{% block body %}
<div class="col-11 mx-auto">
    <h3 class="quotes p-4 shadow">Entrée</h3>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ starter.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ starter.name }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ starter.price }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_starter_index') }}">retour à la liste</a>

    <a href="{{ path('app_starter_edit', {'id': starter.id}) }}">edit</a>

    {{ include('starter/_delete_form.html.twig') }}
</div>
{% endblock %}
